<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="402.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <Label fx:id="isConnected" layoutX="-1.0" layoutY="382.0" prefHeight="16.0" prefWidth="455.0" text="Status:" textFill="#ff5e5e">
         <font>
            <Font name="Roboto Bold" size="14.0" />
         </font>
      </Label>
      <JFXTextField fx:id="loginName" layoutX="129.0" layoutY="107.0" prefHeight="32.0" prefWidth="197.0" promptText="User Name">
         <font>
            <Font name="SansSerif Regular" size="14.0" />
         </font></JFXTextField>
      <JFXTextField fx:id="loginPassword" layoutX="129.0" layoutY="167.0" prefHeight="32.0" prefWidth="197.0" promptText="Password">
         <font>
            <Font name="SansSerif Regular" size="14.0" />
         </font></JFXTextField>
      <JFXButton layoutX="128.0" layoutY="227.0" onAction="#onLogin" prefHeight="32.0" prefWidth="198.0" style="-fx-background-color: #00b894;" text="Login">
         <font>
            <Font name="SansSerif Regular" size="14.0" />
         </font></JFXButton>
   </children>
</AnchorPane>
